@model List<MediSync.Models.ScheduleViewAdminModel>

@{
    ViewBag.Title = "ViewScheduleAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3 class="text-center p-md-2">View All Schedule</h3>

<table class="table table-striped table-hover text-center">
    <thead>
        <tr>
            <th>Patient Name</th>
            <th>Physician Name</th>
            <th>Appointment Date</th>
            <th>Appointment Reason</th>
            <th>Schedule Date Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var schedule in Model)
        {
            <tr>
                <td>@schedule.PatientName</td>
                <td>@schedule.PhysicianName</td>
                <td>@schedule.AppointmentDate</td>
                <td>@schedule.AppointmentReason</td>
                <td>@schedule.ScheduleDateTime</td>
            </tr>
        }
    </tbody>
</table>